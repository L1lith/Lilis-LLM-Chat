{
  "name": "lilis-llm-chat",
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build:nwprep && .\\SDK\\nw.exe ./dist",
    "astro:defaultdev": "astro dev",
    "build:nwprep": "node ./NWJS/prepNW.js && astro build && node ./NWJS/buildNW.js",
    "build": "npm run build:nwprep && node ./NWJS/runBuilder.js && cd ./bundle && npm install",
    "preview": "astro preview",
    "astro": "astro",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "L1lith",
  "license": "ESMIT",
  "description": "An LLM Chat UI by Lili",
  "nwbuild": {
    "mode": "build",
    "glob": false,
    "platform": "win",
    "arch": "x64",
    "flavor": "sdk",
    "srcDir": "./dist",
    "outDir": "./bundle",
    "logLevel": "error",
    "window": {
      "icon": "./NWJS/llm-chat-logo.ico"
    },
    "app": {
      "icon": "./NWJS/llm-chat-logo.ico"
    }
  },
  "icons": {
    "256": "favicon.png"
  },
  "devDependencies": {
    "astro": "^5.12.8",
    "nw-builder": "^4.13.16"
  },
  "dependencies": {
    "@astrojs/solid-js": "^5.1.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "jabr": "^0.3.3",
    "marked": "^16.1.2",
    "node-time-ago": "^1.0.0",
    "open-file-explorer": "^1.0.2",
    "openai": "^5.11.0",
    "sass": "^1.89.2",
    "solid-js": "^1.9.7"
  }
}
